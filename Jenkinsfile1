pipeline {
    agent any
    environment {
		 SCANNER_HOME= tool 'SonarCloud'
    }
    stages {
        stage('Build') {
            steps {
                sh '''
                echo "${SCANNER_HOME}"
                '''
                withSonarQubeEnv('sonarDOTT') {
                sh 'cd ./node'
                sh '${SCANNER_HOME}/bin/sonar-scanner'
                }
            }
        }
     }
  }
