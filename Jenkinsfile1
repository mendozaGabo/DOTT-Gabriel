pipeline {
    environment {

        scannerHome = tool 'sonarcloud';

    }
    
    agent any

    stages {
        stages {
        stage('Build') {
            steps {
                sh '''
                echo "${SCANNER_HOME}"
                '''
                withSonarQubeEnv('sonarcloud') {
                sh 'cd ./node'
                sh '${SCANNER_HOME}/bin/sonar-scanner'
                }
                    }

                        }

                }
    }
}
                